{
    "name": "dfs",
    "version": "1.0.0",
    "description": "Diatem's Front-end Skeleton",
    "main": "index.html",
    "scripts": {
        "dev": "NODE_ENV=development webpack-dev-server --open --profile --progress",
        "dev-watch": "NODE_ENV=development webpack-dev-server --open --watch --profile --progress",
        "watch": "NODE_ENV=development webpack --watch --profile --progress",
        "build": "NODE_ENV=production webpack",
        "build-dev": "NODE_ENV=development webpack --profile --progress",
        "prod": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "CSS",
        "Sass",
        "Responsive",
        "Grid",
        "Layout",
        "Flexbox"
    ],
    "license": "MIT",
    "dependencies": {
        "choices.js": "^9.0.1",
        "normalize.css": "^8.0.1",
        "select2": "^4.0.13"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "autoprefixer": "^9.7.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.0.6",
        "browser-sync": "^2.26.13",
        "browser-sync-webpack-plugin": "^2.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.2",
        "cssnano": "^4.1.10",
        "eslint": "^7.0.0",
        "eslint-loader": "^4.0.2",
        "imagemin-webpack-plugin": "^2.4.2",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.1",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^8.0.2",
        "svg-sprite-loader": "^4.2.1",
        "svgo": "^1.3.2",
        "svgo-loader": "^2.2.1",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "rules": {}
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {},
            "cssnano": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
